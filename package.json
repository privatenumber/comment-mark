{
	"name": "comment-mark",
	"version": "0.0.0-semantic-release",
	"description": "Interpolate strings with HTML comment markers",
	"keywords": [
		"string",
		"interpolation",
		"inject",
		"html",
		"comment",
		"markdown"
	],
	"license": "MIT",
	"repository": "privatenumber/comment-mark",
	"funding": "https://github.com/privatenumber/comment-mark?sponsor=1",
	"author": {
		"name": "Hiroki Osame",
		"email": "hiroki.osame@gmail.com"
	},
	"files": [
		"dist"
	],
	"main": "dist/comment-mark.js",
	"module": "dist/comment-mark.esm.js",
	"scripts": {
		"lint": "eslint .",
		"test": "jest",
		"build": "rollup -c --environment NODE_ENV:production"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint",
			"jest --bail --findRelatedTests"
		]
	},
	"devDependencies": {
		"@babel/preset-env": "^7.12.1",
		"@pvtnbr/eslint-config": "^0.4.1",
		"@skypack/package-check": "^0.1.0",
		"babel-jest": "^26.6.0",
		"core-js": "^3.6.5",
		"eslint": "^8.2.0",
		"husky": "^4.3.0",
		"jest": "^26.6.0",
		"lint-staged": "^10.4.2",
		"rollup": "^2.32.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-filesize": "^9.0.2",
		"rollup-plugin-terser": "^7.0.2"
	},
	"eslintConfig": {
		"extends": "@pvtnbr"
	}
}
